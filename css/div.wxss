/* 顶部隐藏返回按钮 */
.div {
  width: 100%;
  height: 135rpx;
  background-color: white;
  position: fixed;
  top: 0;
}

.div view {
  display: flex;
  align-items: center;
  margin: 80rpx 0 0 20rpx;
}

.div view image {
  width: 20rpx;
  height: 35rpx;
}

.div view text {
  margin-left: 20rpx;
}