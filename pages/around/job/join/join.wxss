.youbo {
  padding: 20rpx;
  letter-spacing: 1px;
}

.top_yb {
  font-size: 30rpx;
  font-weight: bold;
  text-align: center;
  margin-bottom: 30px;
}

.content {
  font-size: 28rpx;
  margin-top: 10rpx;
}

.commend {
  font-size: 28rpx;
  margin-top: 20px;
  display: flex;
  flex-direction: column;

}

.contact {
  margin-top: 30px;
  font-size: 28rpx;
  display: flex;
  flex-direction: column;
}

.yb_img {
  margin-top: 100px;
  display: flex;
  justify-content: flex-end;
  right: 20px;
  font-size: 28rpx;
}

.yb_img view {
  height: 110px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.yb_img image {
  width: 120rpx;
  height: 120rpx;
}